#
# syntax:
#
# module <class_name> "<ztcl_class_name>"
# {
#      export <method_name1> "<ztcl_method_name1>" ( arg1, ..., argN ) ;
#      .
#      .
#      .
#      export <method_nameN> "<ztcl_method_nameN>" ( args ) ;
# }
#
# arg:     argDir argType <arg_name> argOptions
#
# argDir:  in | out
#
# argType: int | float | string | bool
#
# argOptions: [ default = <value>; usage = "tcl_usage"; ]
#
# Example:
#
%{
#include "test.h"
%}

   /** xxxx
this is a test comment
dddd */
module Test "test"
{
    export bool var_bool [ usage = "foo"; notify = vchange; ];
    export int var_int;
    export float var_float;
    export dbobject var_dbobject;
    export zobject var_zobject;
    export string var_string;

    constructor 
    (
        in string name = "zfunc" [ usage = "zfunc s1"; ],
        in int x       = 1000    [ usage = "x coord"; ],
        in float y     = 100.01  [ usage = "y cord"; ],
        in bool skip             [ usage = "skip value"; ]
    );

    export m1 "m1"
    ( 
        in string name = "zfunc" [ usage = "zfunc s1";  ],
        in int x       = 1000    [ usage = "x coord"; ],
        in float y     = 100.01  [ usage = "y cord"; ],
        in bool skip   = false   [ usage = "skip value"; ],
        in rect rin [ usage = "rect"; ],
        in point pin [ usage = "point"; ],

        out string oname = "zfunc" [ usage = "zfunc s1"; ],
        out int ox       = 1000    [ usage = "x coord"; ],
        out float oy     = 100.01  [ usage = "y cord"; ],
        out bool oskip   = false   [ usage = "skip value"; ],
        in rect rout [ usage = "rect"; ],
        in point pout [ usage = "point"; ]
    );

    export m2 "m2"
    ( 
        in string name = "zfunc" [ usage = "zfunc s1"; ],
        in int x       = 1000    [ usage = "x coord"; ],
        in float y     = 100.01  [ usage = "y cord"; ],
        in bool skip             [ usage = "skip value"; ]
    );

    export m3 "m3"
    ( 
        in string name = "zfunc" [ usage = "zfunc s1"; ],
        in int x       = 1000    [ usage = "x coord"; ],
        in float y     = 100.0   [ usage = "y cord"; ],
        in bool skip             [ usage = "skip value"; ],
        in ichannel file          [ usage = "skip value"; ]
    );

    export m4 "m4"
    ( 
        in list string name [ usage = "zfunc s1"; ],
        in list int x       [ usage = "x coord"; ],
        in list float y     [ usage = "y cord"; ],
        in list ichannel ifile [ usage = "skip value"; ],
        in list ochannel ofile [ usage = "skip value"; ]
    );

    export m5 "m5"
    ( 
        out list string name [ usage = "zfunc s1"; ],
        out list int x       [ usage = "x coord"; ],
        out list float y     [ usage = "y cord"; ]
    );
}
